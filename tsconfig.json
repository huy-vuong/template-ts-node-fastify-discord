{
  "compilerOptions": {
    "lib": ["ES2022"],
    "outDir": "./build",
    "module": "CommonJS",
    "target": "ES2022",
    "importHelpers": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "allowJs": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "template-ts-node-fastify/*": ["./src/*"]
    }
  }
}
